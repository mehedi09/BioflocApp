                                        android:id="@+id/btnMaths"
                                        android:background="@color/colorTransparent"
                                        android:onClick="calculator_click"></Button>
                                </RelativeLayout>
                            </androidx.cardview.widget.CardView>
                            <androidx.cardview.widget.CardView
                                android:layout_width="0dp"
                                android:layout_height="match_parent"
                                android:layout_weight="1"
                                android:layout_gravity="center"
                                android:layout_marginLeft="5dp"
                                app:cardCornerRadius="8dp"
                                >
                                <RelativeLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent">
                                    <ImageView
                                        android:layout_width="50dp"
                                        android:layout_height="50dp"
                                        android:id="@+id/youtube"
                                        android:src="@drawable/youtube"
                                        android:layout_centerInParent="true"
                                        >

                                    </ImageView>
                                    <TextView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:text="@string/dboard_item_4"
                                        android:textSize="16dp"
                                        android:layout_below="@+id/youtube"
                                        android:layout_centerHorizontal="true"
                                        android:layout_marginTop="10dp"
                                        android:layout_marginBottom="10dp">

                                    </TextView>
                                    <Button
                                        android:layout_width="match_parent"
                                        android:layout_height="match_parent"
                                        android:id="@+id/btnYoutube"
                                        android:background="@color/colorTransparent"
                                        android:onClick="video_click"></Button>
                                </RelativeLayout>
                            </androidx.cardview.widget.CardView>

                        </LinearLayout>

                        <!---->

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="135dp" android:orientation="horizontal"
                            android:layout_marginBottom="10dp">
                            <androidx.cardview.widget.CardView
                                android:layout_width="0dp"
                                android:layout_height="match_parent"
                                android:layout_weight="1"
                                android:layout_gravity="center"
